<!DOCTYPE html>
<html>
<head>
  <title>view file</title>
  <style>
  	.img-holder {
  		height:100vh; width:100%;
  		background-color: green;
  	}
  </style>
</head>
<body>
  <h1>file</h1>
  <divclass="img-holder"></div>
  <img src="data:image/jpeg;base64, <%= file %>" alt="Decrypted Image">
<script>
	// Assuming you have the binary file in a variable called 'binaryData'
const binaryData = file


  const img = document.createElement('img');


  img.src = new Uint8Array(binaryData)

  // Append the image element to a container in the DOM
  const container = document.getElementById('img-holder');
  container.appendChild(img);
};


</script>
</body>
</html>
